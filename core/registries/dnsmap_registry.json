  "examples": [
    "dnsmap example.com",
    "dnsmap -w wordlist.txt example.com",
    "dnsmap -r example.com"
  ],
  "common_flags": {
    "-w": "Wordlist file",
    "-r": "Reverse DNS lookup",
    "-t": "Number of threads",
    "-o": "Output file"
  },
  "output_format": "text",
  "installation": "apt install dnsmap",
  "documentation": "https://github.com/makefu/dnsmap"
}
